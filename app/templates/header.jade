.container
  .navbar-header
    button.navbar-toggle.collapsed(type="button", data-toggle="collapse", data-target=".header-collapse")
      i.fa.fa-bars
    a.navbar-brand(ui-sref="root.home")
      i.sprite-icon.agora-logo-icon
  nav.navbar-collapse.header-collapse.collapse.in(role="navigation", aria-expanded="true")
    ul.nav.navbar-nav
      li
        a(ui-sref="root.product") product
      li
        a(ui-sref="root.solutions") solutions
      li
        a(href="#section-pricing", ag-link-scroll=true) pricing
      li
        a(ui-sref="root.docs") docs
      li
        a(ui-sref="root.help") help
