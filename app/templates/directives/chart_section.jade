section
  h3.chart-title {{chart.title}} {{query.interval | intervalDisplay}}
  .chart-container
    .controls
      div(ag-interval-switch="")
    .canvas-container
      canvas
      .loader(ng-show="loading")
        i.fa.fa-spin.fa-spinner.fa-4x
      .data-empty-state(ng-hide="hasData")
        i.fa.fa-folder-open-o
        span &nbsp; No data yet
